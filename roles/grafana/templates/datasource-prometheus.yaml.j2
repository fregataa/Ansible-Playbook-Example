# Prometheus Datasource Provisioning
# Generated by Ansible

apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    url: http://localhost:{{ prometheus_port }}
    isDefault: true
    editable: false
    jsonData:
      httpMethod: POST
      manageAlerts: false
      prometheusType: Prometheus
      prometheusVersion: "{{ prometheus_version }}"
      timeInterval: "{{ prometheus_scrape_interval }}"
